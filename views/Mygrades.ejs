        <div class="row">
      
          <div class="col-6 col-md-4">
            <script src="https://cdn.plot.ly/plotly-latest.min.js"></script>
            
            
            <div id="myPlot" style="width:100%;max-width:400px"></div>
            
            <script>
              const xArray = <%- JSON.stringify(xValues); %>;
              const yArray = <%- JSON.stringify(yValues); %>;
            
              const data = [{
                x: xArray,
                y: yArray,
                type: "bar",
                marker: {
                  color: ['rgba(255,0,0,0.5)', 'rgba(0,255,0,0.5)', 'rgba(0,0,255,0.5)', 'rgba(255,255,0,0.5)', 'rgba(0,255,255,0.5)']
                }
              }];
            
              const layout = {
                title: "Average Grade Performance per Subject Bar",
                xaxis: {title: "Subjects"},
                yaxis: {title: "Average Performance"}
              };
            
              Plotly.newPlot("myPlot", data, layout);
            </script>
            



            
          </div>
          <div class="col-6 col-md-4" >
            <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.js"></script>
          
            <canvas id="myChart" style="width:100%;max-width:700px; margin-top: 30%;"></canvas>
            
            <script>
              const xValues = <%- JSON.stringify(xValues); %>;
              const yValues = <%- JSON.stringify(yValues); %>;
                        
              new Chart("myChart", {
                type: "line",
                data: {
                  labels: xValues,
                  datasets: [{
                    fill: true,
                    lineTension: 0,
                    backgroundColor: "rgba(135,206,235,1.0)",
                    borderColor: "rgba(0,0,395,0.1)",
                    data: yValues
                  }]
                },
                options: {
                  legend: {display: false},
                  scales: {
                    yAxes: [{ticks: {min: 0, max: 100}}],
                  },
                  title: {
                    display: true,
                    text: "Average Grade Performance per Subject Line"
                  }
                }
              });
            </script></div>
          <div class="col-6 col-md-4">

            <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.js"></script>
<body>

